    "@fullcalendar/interaction": "^5.0.0",
    "@fullcalendar/list": "^5.0.0",
    "@fullcalendar/moment": "^5.1.0",
    "@fullcalendar/moment-timezone": "^5.1.0",
    "@fullcalendar/rrule": "^5.1.0",
    "@fullcalendar/timegrid": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
